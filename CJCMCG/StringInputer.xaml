<Window x:Class="CJCMCG.StringInputer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CJCMCG"
        mc:Ignorable="d"
        Title="{DynamicResource Savv}" Height="120" Width="200" ResizeMode="NoResize">
    <Grid>
        <TextBox x:Name="str" Margin="10,10,10,45" AcceptsReturn="False"/>
        <Button Content="{DynamicResource Save}" Margin="10,0,10,10" VerticalAlignment="Bottom" Height="25" Click="Button_Click"/>
    </Grid>
</Window>
